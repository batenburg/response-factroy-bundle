{
  "name": "batenburg/response-factory-bundle",
  "description": "A response factory bundle, to clean your controller code.",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Batenburg",
      "email": "p.batenurg@live.nl"
    }
  ],
  "require": {
    "php": ">= 7.2.0",
    "symfony/dependency-injection": "^4.3 || ^4.4 || ^5.0",
    "symfony/framework-bundle": "^4.3 || ^4.4 || ^5.0",
    "symfony/twig-bundle": "^5.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Batenburg\\ResponseFactoryBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Batenburg\\ResponseFactoryBundle\\Test\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
